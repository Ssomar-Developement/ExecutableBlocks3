  <modelVersion>4.0.0</modelVersion>
  <groupId>com.ssomar</groupId>
  <artifactId>ExecutableBlocks</artifactId>
  <name>ExecutableBlocks</name>
  <version>2.1.5.5</version>
  <build>
    <sourceDirectory>${basedir}/src/main/java</sourceDirectory>
    <resources>
      <resource>
        <targetPath>.</targetPath>
        <filtering>true</filtering>
        <directory>${basedir}/src/main/resources/</directory>
        <includes>
          <include>*.yml</include>
        </includes>
      </resource>
      <resource>
        <targetPath>com/ssomar/executableblocks/configs/locale</targetPath>
        <filtering>true</filtering>
        <directory>${basedir}/src/main/resources/locale/</directory>
        <includes>
          <include>Locale*.yml</include>
        </includes>
      </resource>
      <resource>
        <targetPath>com/ssomar/executableblocks/configs/blocks</targetPath>
        <directory>${basedir}/src/main/resources/blocks/</directory>
      </resource>
    </resources>
    <finalName>${project.artifactId}-${project.version}</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.7.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.1.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <filters>
            <filter>
              <artifact>*:*</artifact>
              <excludes>
                <exclude>META-INF/maven/**</exclude>
              </excludes>
            </filter>
          </filters>
          <relocations>
            <relocation>
              <pattern>org.bstats</pattern>
              <shadedPattern>com.ssomar.executableitems</shadedPattern>
            </relocation>
          </relocations>
        </configuration>
      </plugin>
    </plugins>
  </build>
